<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.steven.androidjetpack">

    <application
        android:name=".BasicApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".room.UserInfoActivity"/>
        <activity android:name=".DataBinding.FoodActivity"/>
        <activity android:name=".navigation.NavigationActivity">
        </activity>
        <activity android:name=".workManager.WorkManagerActivity">
        </activity>
        <activity android:name=".DataBinding.FoodListActivity">
        </activity>
        <activity android:name=".DataBinding.TwoWayActivity">
        </activity>
    </application>

</manifest>